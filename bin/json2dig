#!/usr/bin/env node

'use strict';

const read = require('./read');
const {Message} = require('../lib/wire');

const now = Date.now();

read(async (input) => {
  const json = JSON.parse(input);
  const msg = Message.fromJSON(json);
  const str = msg.toString(now);

  process.stdout.write(str + '\n');
});
